@using EconomicUI.Models

<h1>Search Results:</h1>

<ol>
@foreach (State state in Model)
{
    <ul>
        <li>ID: @state.StateId</li>
        <li>Name: @state.Name</li>
        <li>GDP: @state.GDP</li>
        <li>Main Import: @state.MainImport</li>
        <li>Main Export: @state.MainExport</li>
    </ul>
    <a href="/States/Details/@(state.StateId)"><p>View Details</p></a>
    <br>
}
</ol>